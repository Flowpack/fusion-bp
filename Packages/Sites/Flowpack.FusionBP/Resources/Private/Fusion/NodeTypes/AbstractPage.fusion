prototype(Flowpack.FusionBP:AbstractPage) < prototype(Page) {
    head {
        headTags = ${'<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">'}
        stylesheets {
            fonts = Neos.Fusion:Tag {
                tagName = 'link'
                attributes {
                    href = 'http://fonts.googleapis.com/css?family=Raleway:400,100,200,300,500,600,700,800,900|Montserrat:400,700'
                    type = 'text/css'
                    rel = 'stylesheet'
                }
            }
            fontawesome = Neos.Fusion:Tag {
                tagName = 'link'
                attributes {
                    href = Neos.Fusion:ResourceUri {
                        path = 'resource://Flowpack.FusionBP/Public/css/font-awesome.min.css'
                    }
                    type = 'text/css'
                    rel = 'stylesheet'
                }
            }
            animate = Neos.Fusion:Tag {
                tagName = 'link'
                attributes {
                    href = Neos.Fusion:ResourceUri {
                        path = 'resource://Flowpack.FusionBP/Public/css/animate.css'
                    }
                    type = 'text/css'
                    rel = 'stylesheet'
                }
            }
            bootstrap = Neos.Fusion:Tag {
                tagName = 'link'
                attributes {
                    href = Neos.Fusion:ResourceUri {
                        path = 'resource://Flowpack.FusionBP/Public/css/bootstrap.min.css'
                    }
                    type = 'text/css'
                    rel = 'stylesheet'
                }
            }
            main = Neos.Fusion:Tag {
                tagName = 'link'
                attributes {
                    href = Neos.Fusion:ResourceUri {
                        path = 'resource://Flowpack.FusionBP/Public/css/main.css'
                    }
                    type = 'text/css'
                    rel = 'stylesheet'
                }
            }
        }
    }
    body {
        javascripts {
            jquery = Neos.Fusion:Tag {
                tagName = 'script'
                attributes.src = '//ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js'
            }
            wow = Neos.Fusion:Tag {
                tagName = 'script'
                attributes.src = Neos.Fusion:ResourceUri {
                    path = 'resource://Flowpack.FusionBP/Public/js/wow.min.js'
                }
            }
            bootstrap = Neos.Fusion:Tag {
                tagName = 'script'
                attributes.src = Neos.Fusion:ResourceUri {
                    path = 'resource://Flowpack.FusionBP/Public/js/bootstrap.min.js'
                }
            }
            main = Neos.Fusion:Tag {
                tagName = 'script'
                attributes.src = Neos.Fusion:ResourceUri {
                    path = 'resource://Flowpack.FusionBP/Public/js/main.js'
                }
            }
        }
    }
}
