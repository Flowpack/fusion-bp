        tagName = 'span'
        content = ${q(node).property('amount')}
        @process.contentElementEditable = ContentElementEditable {
            property = 'amount'
        }
    }
    projects = Neos.Fusion:Tag {
        tagName = 'span'
        content = ${q(node).property('projects')}
        @process.contentElementEditable = ContentElementEditable {
            property = 'projects'
        }
    }
    storage = Neos.Fusion:Tag {
        tagName = 'span'
        content = ${q(node).property('storage')}
        @process.contentElementEditable = ContentElementEditable {
            property = 'storage'
        }
    }
