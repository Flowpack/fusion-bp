        tagName = 'span'
        content = ${q(node).property('title')}
        @process.contentElementEditable = ContentElementEditable {
            property = 'title'
        }
    }
    info = Neos.Fusion:Tag {
        tagName = 'span'
        content = ${q(node).property('info')}
        @process.contentElementEditable = ContentElementEditable {
